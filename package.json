{
  "name": "iot-cms",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "yarn ms:flags:silent && yarn ms:translations:silent && vite",
    "build": "tsc && vite build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.json .",
    "preview": "vite preview",
    "ms:translations": "cd ./translations-management && yarn build && yarn preview",
    "ms:translations:silent": "yarn ms:translations:silent:windows || yarn ms:translations:silent:unix",
    "ms:translations:silent:windows": "start /B yarn ms:translations",
    "ms:translations:silent:unix": "yarn ms:translations &",
    "ms:flags": "cd ./feature-flags && yarn build && yarn preview",
    "ms:flags:silent": "yarn ms:flags:silent:windows || yarn ms:flags:silent:unix",
    "ms:flags:silent:windows": "start /B yarn ms:flags",
    "ms:flags:silent:unix": "yarn ms:flags &"
},
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "classnames": "^2.5.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-markdown": "^9.0.1",
    "react-markdown-editor-lite": "^1.3.4",
    "react-router-dom": "^6.21.1",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.16.1",
    "@iot/portal-core-components": "5.5.8",
    "@originjs/vite-plugin-federation": "^1.3.5",
    "@types/eslint": "^8.44.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-helmet": "^6.1.11",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^5.2.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-tailwindcss": "^3.3.6",
    "eslint-plugin-testing-library": "^6.2.0",
    "postcss": "^8.4.32",
    "prettier": "2.5.1",
    "sass": "^1.69.7",
    "tailwindcss": "^3.4.0",
    "typescript": "5.0.2",
    "vite": "^5.0.8"
  }
}
